  <div class = 'gill w-70per mrg-auto min-w-350px max-w-900px'>

    <h1 class = "ft-lite ft-sz-2 mrg-btm-0 txt-ctr brd-bt pad-bt-5"><%= @data[:student].first_name %> <%= @data[:student].last_name %> OverView</h1>
    <p>LOTS OF DATA HERE</p>
    <% if current_user.user_type == 'student' %>
    <% @data[:submissions].each do |entry| %>
    <h3 class = "hov-grn ft-lite ft-sz-1half brd-bt pad-bt-5 mrg-btm-0 pad-top-15"><a class = "no-txt-dec black hov-grn" href="<%= classroom_path(entry[:klass]) %>"><%= Classroom.find(entry[:klass]).name %></a></h3>
    <!-- Visit Class Page (put as a link for class) -->



    <table class = "w-100per brd-clpse">
      <tr class = "ft-lite ft-sz-1half mrg-top-45 mrg-btm-0 brd-bt pad-bt-5 green">
        <th class = "ft-lite txt-lt w-50per">Assignment</th>
        <th class = "ft-lite txt-ctr">Status</th>
        <th class = "ft-lite txt-rt">Due Date</th>
      </tr>
      <%  entry[:submissions].each do |sub| %>
      <tr onclick="document.location = '<%= submission_path(sub.id) %>'" class = 'ft-lite mrg-l-n40 ft-sz-1q hov-bkg-grn'>
        <td class = "ft-lite pad-top-15 pad-l-5 txt-lt"><%= sub.assignment.title %></td>
        <td class = "ft-lite pad-top-15 txt-ctr">Level</td>
        <td class = "ft-lite pad-top-15 pad-r-5 txt-rt">today!</td>
      </tr>
      <% end %>
    </table>






    <% end %>





    <% elsif current_user.user_type == 'teacher' %>

    <% if @data[:submissions][0] %>
    <!-- <h3 class = "hov-grn ft-lite ft-sz-1half brd-bt pad-bt-5 mrg-btm-0 pad-top-15">Classes You Teach</h3> -->
    <% @data[:submissions].each do |entry| %>
    <h4 class = "hov-grn ft-lite ft-sz-1half brd-bt pad-bt-5 mrg-btm-0 pad-top-15"><a class = "no-txt-dec black hov-grn" href="<%= classroom_path(entry[:klass]) %>"><%= Classroom.find(entry[:klass]).name %></a></h4>
    <!-- <p><%= @data[:student].first_name %>'s Submissions: </p> -->


    <table class = "w-100per brd-clpse">
      <tr class = "ft-lite ft-sz-1half mrg-top-45 mrg-btm-0 brd-bt pad-bt-5 green">
        <th class = "ft-lite txt-lt w-50per">Assignment</th>
        <th class = "ft-lite txt-ctr">Status</th>
        <th class = "ft-lite txt-rt">Due Date</th>
      </tr>
      <%  entry[:submissions].each do |sub| %>
      <tr onclick="document.location = '<%= submission_path(sub.id) %>'" class = 'ft-lite mrg-l-n40 ft-sz-1q hov-bkg-grn'>
        <td class = "ft-lite pad-top-15 pad-l-5 txt-lt"><%= sub.assignment.title %></td>
        <td class = "ft-lite pad-top-15 txt-ctr">Level</td>
        <td class = "ft-lite pad-top-15 pad-r-5 txt-rt">today!</td>
      </tr>
      <% end %>
    </table>
    <% end %>


    <% else %>
    <h3 class = "hov-grn ft-lite ft-sz-1half brd-bt pad-bt-5 mrg-btm-0 pad-top-15">This student is not in any of your current classes.</h3>
    <% end %>
    <% end %>

  </div>
