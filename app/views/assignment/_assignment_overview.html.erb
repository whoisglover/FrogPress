  <h5><a href = "<%= edit_assignment_path(@assignment.id) %>">Edit Assignment</a></h5>
  <table>
    <tr>
      <th>Student</th>
      <th>Level</th>
      <th>Status</th>
    </tr>
    <%@students.each do |student|%>
    <tr>
      <td><%= student.first_name %> <%= student.last_name %></td>
      <td>Level</td>
      <td>Assignment Status</td>
    </tr>
    <% end %>
  </table>
